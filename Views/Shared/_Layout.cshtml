<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - RRS</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/RRS.styles.css" asp-append-version="true" />

    <!-- Bootstrap core JavaScript-->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.3.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/2.2.1/css/dataTables.bootstrap5.css">


    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css">

    <!-- Boxicons CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/boxicons@latest/css/boxicons.min.css">

    <!-- jQuery -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

    <!-- Bootstrap Bundle JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"></script>


    <!-- Core plugin JavaScript-->
    <script src="vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="js/sb-admin-2.min.js"></script>

    <!-- Page level plugins -->
    <script src="vendor/datatables/jquery.dataTables.min.js"></script>
    <script src="vendor/datatables/dataTables.bootstrap4.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="js/demo/datatables-demo.js"></script>

    <!-- CSS FILES -->
    <link href="~/css/adminLayout.css" rel="stylesheet" />

    <!-- BOXICONS -->
    <link href="https://cdn.jsdelivr.net/ npm/boxicons@2.0.7/css/boxicons.min.css" rel="stylesheet" />

    <!-- JS FILES -->
    <script defer src="~/js/adminLayout.js"></script>

</head>

<body id="body-pd" class="bg-light">
    <header class="header bg-white shadow-sm" id="header" style="height: 50px;">
        <div class="header_toggle">
            <i class='bx bx-menu' id="header-toggle"></i>
        </div>
        <div class="header_img">
            <img src="" alt="img">
        </div>
    </header>

    <div class="l-navbar" id="nav-bar">
        <nav class="nav">
            <div>
                <a class="nav_logo" asp-area="" asp-action="Index" asp-controller="Dashboard">
                    <i class='bx bx-dish nav_logo-icon'></i>
                    <span class="nav_logo-name">Golden Palate</span>
                </a>
                <div class="nav_list">
                    <a class="nav_link active" asp-area="" asp-action="Index" asp-controller="Dashboard">
                        <i class='bx bx-grid-alt nav_icon'></i>
                        <span class="nav_name">Dashboard</span>
                    </a>
                    <a  class="nav_link" asp-area="" asp-action="Index" asp-controller="Reservation">
                        <i class='bx bx-calendar nav_icon'></i>
                        <span class="nav_name">Reservations</span>
                    </a>
                    <a  class="nav_link" asp-area="" asp-action="Index" asp-controller="Menu">
                        <i class='bx bx-food-menu nav_icon'></i>
                        <span class="nav_name">Menu Management</span>
                    </a>
                    <a class="nav_link" asp-area="" asp-action="Index" asp-controller="Table">
                        <i class='bx bx-table nav_icon'></i>
                        <span class="nav_name">Table Management</span>
                    </a>
                    <a class="nav_link" asp-area="" asp-action="Index" asp-controller="AuditLogs">
                        <i class='bx bx-history nav_icon'></i>
                        <span class="nav_name">Audit Logs</span>
                    </a>
                </div>
            </div>
            <a href="#" class="nav_link">
                <i class='bx bx-log-out nav_icon'></i>
                <span class="nav_name">SignOut</span>
            </a>
        </nav>
    </div>

    <!--Container Main start-->
    <div class="height-100" style="margin-top: 65px;">
        @RenderBody()
    </div>
    <!--Container Main end-->

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script src="https://code.jquery.com/jquery-3.7.1.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.3.0/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdn.datatables.net/2.2.1/js/dataTables.js"></script>
    <script src="https://cdn.datatables.net/2.2.1/js/dataTables.bootstrap5.js"></script>


    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
